#include <ESP8266WiFi.h>
#include <ESPAsyncTCP.h>
#include <ESPAsyncWebServer.h>

// Set Wifi router login details.
const char* ssid = "The Nest"; const char* password = "HummyNest"; IPAddress STAIP(192,168,0,27); IPAddress STAgateway(192,168,0,1); IPAddress STAsubnet(255,255,255,0); // Set STA mode IP and gateway address

WiFiEventHandler wifiConnectHandler; WiFiEventHandler wifiDisconnectHandler;

const int motor1 = 4; const int motor2 = 5; // Set gpio pins for each motor. ESP8266 Startup safe pins are 4, 5.
const int GATE1 = 14; // PLACEHOLDER gpio to check gate closed status. N.O. sensor tied to 0V.
const int GARAGE1 = 12; // PLACEHOLDER gpio to check garage closed status. N.O. sensor tied to 0V.

AsyncWebServer server(80); // SET the port desired

String readgpio14() { bool t1 = digitalRead(GATE1); if (t1 == 1) { return String("."); } else { return String("CLOSED"); } } // PLACEHOLDER for gpio reading into the variable. Set OPEN in the double quotes for a sensor installed.
String readgpio12() { bool t1 = digitalRead(GARAGE1); if (t1 == 1) { return String("."); } else { return String("CLOSED"); } } // PLACEHOLDER for gpio reading into the variable. Set OPEN in the double quotes for a sensor installed.  

// HTML web page and Set button names
const char index_html[] PROGMEM = R"rawliteral(
<!DOCTYPE HTML><html>
  <head>
    <title>SuperMation Products Limited</title>
    <link rel="shortcut icon" href="data:image/x-icon;base64," />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="refresh" content="120">    
    <style> body { font-family: Arial; text-align: center; margin:0px auto; padding-top: 30px;}
      .button { padding: 10px 20px; font-size: 24px; text-align: center; outline: none; color: #fff; background-color: #2f4468; border: none; border-radius: 5px; box-shadow: 0 6px #999; cursor: pointer; -webkit-touch-callout: none;
      -webkit-user-select: none; -khtml-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; -webkit-tap-highlight-color: rgba(0,0,0,0); }  
      .button:hover {background-color: #1f2e45}
      .button:active { background-color: #1f2e45; box-shadow: 0 4px #666; transform: translateY(2px); } </style>
  </head>  
    <body>
    <h1>AccessMatic<sup><span style="font-size: 10pt;">TM</span></sup></h1>
    <button class="button" ontouchend="motor1()" onmousedown="toggleCheckbox('on1');" ontouchstart="toggleCheckbox('on1');" onmouseup="toggleCheckbox('off1');" ontouchend="toggleCheckbox('off1');">GATE <span style="color:white;font-size:22px;" id="gpio14">%GATE1%</span></span></button>        
    <br></br>
    <button class="button" ontouchend="motor2()" onmousedown="toggleCheckbox('on2');" ontouchstart="toggleCheckbox('on2');" onmouseup="toggleCheckbox('off2');" ontouchend="toggleCheckbox('off2');">GARAGE <span style="color:white;font-size:22px;" id="gpio12">%GARAGE1%</span></span></button>
   <script> function toggleCheckbox(x) { var xhr = new XMLHttpRequest(); xhr.open("GET", "/" + x, true); xhr.send(); } </script>
  </body>

<script> setInterval(function ( ) { var xhttp = new XMLHttpRequest(); // PLACEHOLDER This is for the automatic updating of the gpio
xhttp.onreadystatechange = function() { if (this.readyState == 4 && this.status == 200) { document.getElementById("gpio14").innerHTML = this.responseText; } }; xhttp.open("GET", "/gpio14", true); xhttp.send(); }, 500 ) ; </script>

<script> setInterval(function ( ) { var xhttp = new XMLHttpRequest(); // PLACEHOLDER This is for the automatic updating of the gpio
xhttp.onreadystatechange = function() { if (this.readyState == 4 && this.status == 200) { document.getElementById("gpio12").innerHTML = this.responseText; } }; xhttp.open("GET", "/gpio12", true); xhttp.send(); }, 500 ) ; </script>

</html>)rawliteral";

void notFound(AsyncWebServerRequest *request) { request->send(404, "text/plain", "Not found"); }

String processor(const String& var){ if(var == "GATE1"){ return readgpio14(); } return String(); } // PLACEHOLDER Replaces placeholder with gpio values in the html body
String processor1(const String& var){ if(var == "GARAGE1"){ return readgpio12(); } return String(); } // PLACEHOLDER Replaces placeholder with gpio values in the html body

void initWiFi() { WiFi.mode(WIFI_STA); //WiFi mode
WiFi.config(STAIP, STAgateway, STAsubnet); WiFi.begin(ssid, password); Serial.print("Connecting to WiFi .."); Serial.print("AccessMatic Base model STA IP Address:"); Serial.println(WiFi.localIP()); }
void onWifiConnect(const WiFiEventStationModeGotIP& event) { Serial.println("Connected to Wi-Fi sucessfully."); Serial.println(); Serial.print("AccessMatic Base model STA IP Address:"); Serial.println(WiFi.localIP()); }
void onWifiDisconnect(const WiFiEventStationModeDisconnected& event) { Serial.println("Disconnected from Wi-Fi, trying to connect..."); WiFi.disconnect(); WiFi.begin(ssid, password); }

void setup() { Serial.begin(115200); wifiConnectHandler = WiFi.onStationModeGotIP(onWifiConnect); wifiDisconnectHandler = WiFi.onStationModeDisconnected(onWifiDisconnect); //Register event handlers   
  initWiFi(); 
   
  pinMode(2, OUTPUT); // initialize booted LED  
  pinMode(GATE1, INPUT_PULLUP); pinMode(GARAGE1, INPUT_PULLUP); // Initialize the gates and garages inputs gpio  
  pinMode(motor1, OUTPUT); digitalWrite(motor1, LOW); pinMode(motor2, OUTPUT); digitalWrite(motor2, LOW); // Motors startup logic states

  server.on("/", HTTP_GET, [](AsyncWebServerRequest *request){ request->send_P(200, "text/html", index_html); }); // SET page name, if desired, after the / character  
  server.on("/gpio14", HTTP_GET, [](AsyncWebServerRequest *request){ request->send_P(200, "text/plain", readgpio14().c_str()); }); // PLACEHOLDER This is for the realtime updating of the gpio reading  
  server.on("/gpio12", HTTP_GET, [](AsyncWebServerRequest *request){ request->send_P(200, "text/plain", readgpio12().c_str()); }); // PLACEHOLDER This is for the realtime updating of the gpio reading  
  server.on("/on1", HTTP_GET, [] (AsyncWebServerRequest *request) { digitalWrite(motor1, HIGH); digitalWrite(2, LOW); request->send(200, "text/plain", "ok"); }); // Motor1 ON  
  server.on("/on2", HTTP_GET, [] (AsyncWebServerRequest *request) { digitalWrite(motor2, HIGH); digitalWrite(2, LOW); request->send(200, "text/plain", "ok"); }); // Motor2 ON
  server.onNotFound(notFound); server.begin();
}

void loop() {
}
